        <script>
            document.addEventListener("DOMContentLoaded", function () {
                var accordions = document.getElementsByClassName("faq-page-accordion");
        
                function closeAllAccordions() {
                    for (var i = 0; i < accordions.length; i++) {
                        accordions[i].classList.remove("active");
                        accordions[i].nextElementSibling.style.display = "none";
                        accordions[i].querySelector("svg").style.transform = "rotate(0deg)";
                    }
                }
        
                function toggleAccordion() {
                    var panel = this.nextElementSibling;
                    var isActive = this.classList.contains("active");
        
                    closeAllAccordions();
        
                    if (!isActive) {
                        this.classList.add("active");
                        panel.style.display = "block";
                        this.querySelector("svg").style.transform = "rotate(-90deg)";
                        this.querySelector("svg").style.transition = "0.3s ease-in-out";
                    }
                }
        
                for (var i = 0; i < accordions.length; i++) {
                    accordions[i].addEventListener("click", toggleAccordion);
                }
        
       
                if (accordions.length > 0) {
                    accordions[0].click();
                }
            });
        </script>
        







